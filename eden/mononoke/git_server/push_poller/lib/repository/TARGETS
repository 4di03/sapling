    name = "repository",
    srcs = glob(["src/**/*.rs"]),
    autocargo = {"ignore_rule": True},
    deps = [
        "//common/rust/mysql:mysql_client",
        "//eden/mononoke/mononoke_types:mononoke_types",
    ],
)
